{
  "manifest_version": 3,
  "name": "YouTube Music Downloader",
  "description": "Download music from YouTube Music",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://music.youtube.com/*"   
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "src/popup.html"
  }
}
